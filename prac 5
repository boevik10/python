#Задание 4

mass = [14, -6, 3, 11, 6, 8.5, 99, -20, -6, 10, 40, 0.25, -4, 5]
for a in mass[:]:
    if a < 0:
        mass.remove(a)
mass.sort()
print(mass)
mass.sort(reverse=True)
print(mass)

