# задание 1 
sw1 = False
sw2 = False
ans = input("Включить? ")
if ans.lower() == "да":
    sw1 = True
    sw2 = True
    print("Всё включено", sw1, sw2)
else:
    print(sw1, sw2)

# задание 2 
num9 = int(input("Введите число: "))
if num9 > 0 and num9 % 2 == 0:
    print(True, "even")
elif num9 > 0 and num9 % 2 != 0:
    print(True, "odd")
else:
    print(False)

# задание 3
