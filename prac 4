else:
    print("It's string")


# вариант 10
# задание 1
stroka10 = input("Введите строку: ")
dl = len(stroka10)
if dl == 0:
    print(None)
elif dl <= 5:
    print("short")
elif dl <= 10:
    print("normal")
else:
    print("long")

# задание 2 
chislo10 = int(input("Введите целое число: "))
if chislo10 < 0:
    chislo10 = 1_000_000
    print(chislo10)
elif chislo10 == 0:
    print(2 ** 2)
else:
    print(chislo10 ** 3)

