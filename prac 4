#Задание 2

pc_number = 777
x, y = map(int, input().split())
if (x < pc_number < y) or (y < pc_number < x):
    print(True)
else:
    print(False)
#Задание 3

lamp_1 = 0
lamp_2 = 0
ans = input("Какую лампочку зажечь? ")
if ans == "1":
    lamp_1 = 1
elif ans == "2":
    lamp_2 = 1
else:
    print("Обе лампочки не горят!")
print("lamp_1 =", lamp_1)
print("lamp_2 =", lamp_2)
