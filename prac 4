# Вариант 11

# задание 1
prog_num = 0
a = int(input("Введите первое число: "))
b = int(input("Введите второе число: "))
if a < 0 and b < 0:
    prog_num = a + b
    print(prog_num)
elif a > 0 and b > 0:
    prog_num = a - b
    print(prog_num)
else:
    print(False)

# задание 2
c = int(input("Введите число: "))
if c % 2 != 0:
    print(c + 1)
else:
    print(True)

# задание 3
s = input("Введите строку: ")
if len(s) > 10:
    print(s[:5])
else:
    print(s)


# Вариант 12 

# задание 1
ru = 'абвгдеёжзийклмнопрстуфхцчшщьыъэюя'
en = 'abcdefghijklmnopqrstuvwxyz'
l = input("Введите букву: ")
if l in ru:
    print("rus")
elif l in en:
    print("eng")
else:
    print(None)

# задание 2
pc_num = 10
x = int(input("Введите число: "))
if x == pc_num or x == pc_num - 1 or x == pc_num + 1:
    print(True)
else:
    print(False)

# задание 3
right_ans = (221 - 13) * 2
print(right_ans)
u = int(input("Ваш ответ: "))
if u == right_ans:
    print(True)
elif u > right_ans:
    print(">")
else:
    print("<")
